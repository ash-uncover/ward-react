  },
  "devDependencies": {
    "@babel/cli": "7.22.9",
    "@babel/core": "7.22.9",
    "@babel/preset-env": "7.22.9",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@types/jest": "29.5.3",
    "@types/node": "20.4.2",
    "@types/react-dom": "18.2.7",
    "babel-loader": "9.1.3",
    "concurrently": "8.2.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.8.1",
    "html-webpack-plugin": "5.5.3",
    "node-fetch": "3.3.1",
