{
  "name": "@uncover/ward-react",
  "version": "0.1.21",
  "author": "aSH <ash.uncover@gmail.com>",
  "description": "Ward - React integration",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ash-uncover/ward-react"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "npm run build:tsc && npm run build:babel && npm run build:css",
    "build:babel": "babel src/lib --extensions .ts,.tsx --out-dir dist",
    "build:css": "postcss src/lib/**/*.css --base src/lib --dir dist",
    "build:tsc": "tsc",
    "start": "webpack serve --progress --mode development"
  },
  "dependencies": {
    "@uncover/js-utils": "^0.0.13",
    "@uncover/js-utils-logger": "^0.1.5",
    "@uncover/ward": "^0.2.3"
  },
  "peerDependencies": {
    "@uncover/ward": "^0.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.4",
    "@types/react-dom": "^18.2.4",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.4",
    "html-webpack-plugin": "^5.5.1",
    "node-fetch": "^3.3.1",
    "postcss": "^8.4.23",
    "postcss-cli": "^10.1.0",
    "postcss-loader": "^7.3.1",
    "postcss-preset-env": "^8.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  }
}
